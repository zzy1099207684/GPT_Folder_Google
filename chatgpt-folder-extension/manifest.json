{
  "manifest_version": 3,
  "name": "ChatGPT Folder,Font,Prompt",
  "version": "1.4",
  "description": "a simple and great tool",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*", "https://chatgpt.com/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}
